{
  "name": "matag-admin",
  "version": "1.0.0",
  "description": "matag admin ui",
  "private": true,
  "scripts": {
    "build-dev": "webpack -d",
    "build-prod": "webpack -p",
    "lint": "eslint src/main/js/ src/test/js/ --ext .js --ignore-pattern node_modules/",
    "lint-fix": "eslint src/main/js/ src/test/js/ --ext .js --fix",
    "postinstall": "yarn build-prod",
    "test": "jest",
    "watch": "webpack -d --watch",
    "watch-htmlunit": "webpack -p --watch"
  },
  "author": "Antonio Alonzi",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^10.0.4",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^6.8.0",
    "css-loader": "^3.4.2",
    "eslint": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "fetch-mock": "^9.9.0",
    "jest": "^26.0.1",
    "jest-transform-css": "^2.0.0",
    "node-fetch": "^3.1.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/test/js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css",
      "^admin/(.*)$": "<rootDir>/src/main/js/admin/$1"
    },
    "automock": false
  }
}
